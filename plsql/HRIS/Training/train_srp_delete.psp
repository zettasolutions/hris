BEGIN
   IF p_del_ts.COUNT > 0 THEN
      FOR i IN 1..p_del_ts.COUNT LOOP
         DELETE FROM S004_T07011 WHERE spk_code = p_del_ts(i);
      END LOOP;
   END IF;
END;
