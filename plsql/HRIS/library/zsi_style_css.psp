body{
 font-family: Verdana,Arial,"Verdana","Helvetica",Sans-serif;
 margin-left:1px; 
 margin-right:1px;  
 margin-top:0px; 
 margin-bottom:0px;
 background:#f5f6f6;
}
ul
{
    list-style-type: none;
}

