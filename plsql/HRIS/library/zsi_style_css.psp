/* m5= minus 5*/
.LeftAdjust-m5{
  margin-left:-5px
}

.LeftAdjust-m4{
  margin-left:-4px
}

.LeftAdjust-m3{
  margin-left:-3px
}


/*Tabs*/


.tab-pages div.tab{
  display:none;
  background-color:#fff;
  min-height:200px;
  padding-top:5px;
  padding-left:2px;
  overflow:hidden;
  border-color: transparent #ddd #ddd;
  border-style: solid;
  border-width: 1px;
}

.tab-pages div.active{
  display:block;

}
td #delete-row span{
   margin-top:7px;
}

/*end Tabs */



/*search panel*/
.SearchPanel{

   border: 1pt solid #a6a6a6;
   height: 250px;
   overflow-y: scroll;
   width: 300px;
   position: absolute;
   display:none;
   background:#fff;
}

.SearchPanel table tr.active td{

   background-color:#4285F4;
   color:#fff;
}

.SearchPanel table tr.active td a{

   color:#fff;
}

.SearchPanel table tr:hover td{

   background-color:#94bcff;
}

.SearchPanel tbody tr:nth-child(even) {background: #e5f2f8}
.SearchPanel tbody tr:nth-child(odd) {background: #fff}

/*end of search panel*/


/*gridReport*/

   .fullWidth{
      width:100%;
   }
 
 .gridReport {
      border-collapse: collapse;
      border:1pt;
   }
   
   .gridReport tr th, .gridReport tr td{
      border: solid 1pt #000;      
      padding-left: 5px;
      padding-right: 5px;
   }

   .gridReport tr th{
      font-size:10pt;            
   }
   
   .gridReport tr td{
      font-size:10pt;       
   }
   
/*end of gridReport*/

/* Report */

.reportcriteria{
   font-size:10pt;
   margin-bottom: 3px;
}

.reportTitle{
   font-size: 11pt;
   font-weight: bold;
   margin-bottom: 2px;
   margin-top: 5px;
}
 
.loadingStatus{
   color: red;
   font-size: 10pt;
   font-weight: bold;
   margin: 0 auto 30px;
   width: 220px;   
}

.progressWindow{
   background-color: #fff;
   color: #055A78;
   font-weight: bold;
   height: 25px;
   left: 400px;
   top: 0;
   padding-left: 10px;
   padding-top: 3px;   
   position: absolute;
   width: 200px;
   z-index: 1000;
   display:none;
}

.errorWindow{
   background-color: #fff;
    color: red;
    font-weight: bold;
    left: 400px;
    padding: 5px 5px 10px 10px;
    position: absolute;
    top: 0;
    width: 315px;
    z-index: 1000;
    border: 1pt solid red;
    display:none;
}


.loadingImg{
   background :url("/apps/s004/images/loading.gif") no-repeat;
   width:30px;   
   height:19px;   
   float: left;
}

.loadIconR{
   background-image: url("/apps/s004/images/loading.gif") ;
   background-repeat: no-repeat;
   background-size: 17px 15px;
   background-position: right center;   
}


.loadingText{
   float: left;
   margin-left: 5px;   
}

.alignCenter{
   text-align:center;
}

.alignLeft{
   text-align:left;
}

/*end of Report*/